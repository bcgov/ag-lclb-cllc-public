{
  "title": "Find out what you need to apply for a licence",
  "pages": [
    {
      "name": "q1",
      "elements": [
        {
          "type": "radiogroup",
          "name": "age19",
          "title": "Are you 19 years old or over?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "name": "q2",
      "elements": [
        {
          "type": "radiogroup",
          "name": "registeredBusiness",
          "title": "Do you have a registered business in British Columbia that will be applying for a license?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "q3",
      "elements": [
        {
          "type": "radiogroup",
          "name": "businessType",
          "title": "What type of business will be applying for the licence?",
          "isRequired": true,
          "choices": [
            {
              "value": "corporation",
              "text": "Corporation"
            },
            {
              "value": "partnership",
              "text": "Partnership"
            },
            {
              "value": "soleProprietorship",
              "text": "Sole-Proprietorship"
            },
            {
              "value": "society",
              "text": "Society"
            },
            {
              "value": "coOperative",
              "text": "Co-operative"
            },
            {
              "value": "LgFn",
              "text": "Local Government or First Nation"
            }
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {registeredBusiness} = \"Yes\""
    },
    {
      "name": "q4",
      "elements": [
        {
          "type": "radiogroup",
          "name": "bceidAccount",
          "title": "Do you have a 'Business BCeID' account?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No",
            {
              "value": "doNotknow",
              "text": "I don't know"
            }
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {registeredBusiness} = \"Yes\""
    },
    {
      "name": "q5",
      "elements": [
        {
          "type": "dropdown",
          "name": "storelocation",
          "title": "Where do you plan on opening a store?",
          "isRequired": true,
          "choicesByUrl": {
            "url": "api/jurisdiction",
            "valueName": "name"
          }
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "q6",
      "elements": [
        {
          "type": "radiogroup",
          "name": "locationOwnedLeased",
          "title": "Do you have a store location currently owned or leased?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "q7",
      "elements": [
        {
          "type": "radiogroup",
          "name": "contactLgFn",
          "title": "Have you contacted your local government or First Nation about opening a cannabis store?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "q8",
      "elements": [
        {
          "type": "radiogroup",
          "name": "localBusinessLicenseCannabis",
          "title": "Do you have a local business licence for cannabis sales?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{contactLgFn} = 'Yes'"
    },
    {
      "name": "q9",
      "elements": [
        {
          "type": "radiogroup",
          "name": "connectionToProducerCultivator",
          "title": "Do you have a financial or business connection to a licensed producer or a cultivator planning to become a licensed producer?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    }
  ],
  "goNextPageAutomatic": false
}
