{
  "title": "Find out what you need to apply for a licence",
  "pages": [
    {
      "name": "p1",
      "elements": [
        {
          "type": "radiogroup",
          "name": "age19",
          "title": "Are you 19 years old or older?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "name": "p2",
      "elements": [
        {
          "type": "radiogroup",
          "name": "registeredBusiness",
          "title": "Do you have a registered business or organization?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "p3",
      "elements": [
        {
          "type": "radiogroup",
          "name": "businessType",
          "title": "What type of business or organization is applying for the licence?",
          "isRequired": true,
          "choices": [
            "Corporation",
            "Partnership",
            "Sole-Proprietorship",
            {
              "value": "Society",
              "text": "Society or similar (Legion, Fraternal Organization etc.)"
            },
            "Trust",
            "Estate",
            "Local Government",
            "Indigenous Nation",
            "Other"
          ]
        },
        {
          "type": "radiogroup",
          "name": "corporationType",
          "visible": false,
          "visibleIf": "{businessType} = 'Corporation'",
          "title": "Corporation Type",
          "isRequired": true,
          "choices": [
            {
              "value": "Public Corporation",
              "text": "Public Corporation"
            },
            {
              "value": "Private Corporation",
              "text": "Private Corporation"
            },
            {
              "value": "Limited Liability Corporation",
              "text": "Limited Liability Corporation"
            },
            {
              "value": "Unlimited Liability Corporation",
              "text": "Unlimited Liability Corporation"
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "partnershipType",
          "visible": false,
          "visibleIf": "{businessType} = 'Partnership'",
          "title": "Partnership Type",
          "isRequired": true,
          "choices": [
            {
              "value": "General Partnership",
              "text": "General Partnership"
            },
            {
              "value": "Limited Partnership",
              "text": "Limited Partnership"
            },
            {
              "value": "Limited Liability Partnership",
              "text": "Limited Liability Partnership"
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "otherType",
          "visible": false,
          "visibleIf": "{businessType} = 'Other'",
          "title": "OtherType",
          "isRequired": true,
          "choices": [
            "School/University",
            "Co-op",
            "Strata Council",
            "Church"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {registeredBusiness} = \"Yes\""
    },
    {
      "name": "p4",
      "elements": [
        {
          "type": "radiogroup",
          "name": "bceidAccount",
          "title": "Do you have a Business BCeID account?",
          "choices": [
            "Yes",
            "No",
            {
              "value": "doNotknow",
              "text": "I don't know"
            }
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {registeredBusiness} = \"Yes\""
    },
    {
      "name": "p5",
      "elements": [
        {
          "type": "radiogroup",
          "name": "isIndigenousNationsLand",
          "title": "Will your proposed location be located on Indigenous Nations land?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' "
    },
    {
      "name": "p6",
      "elements": [
        {
          "type": "text",
          "name": "indigenousNationsLand",
          "visible": false,
          "visibleIf": "{isIndigenousNationsLand} = 'Yes'",
          "width": "50%",
          "title": "Which Indigenous Nation will the proposed location be operating in?"
        },
        {
          "type": "radiogroup",
          "name": "contactedFirstNation",
          "visible": false,
          "visibleIf": "{isIndigenousNationsLand} = 'Yes'",
          "title": "Have you contacted the First Nation about opening a cannabis store?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' "
    },
    {
      "name": "p7",
      "elements": [
        {
          "type": "dropdown",
          "name": "storelocation",
          "width": "50%",
          "title": "Which city will the proposed location be operating in?",
          "isRequired": true,
          "choicesByUrl": {
            "url": "api/jurisdiction",
            "valueName": "name"
          }
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "p8",
      "elements": [
        {
          "type": "radiogroup",
          "name": "contactLg",
          "title": "Have you contacted the local government about opening a cannabis store?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "p9",
      "elements": [
        {
          "type": "radiogroup",
          "name": "locationOwnedLeased",
          "title": "Do you have a location currently owned or leased?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "p10",
      "elements": [
        {
          "type": "radiogroup",
          "name": "connectionToProducerCultivator",
          "title": "Does your business or yourself have any business, financial or family connections to licensed producers or processors of cannabis?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    }
  ],
  "showQuestionNumbers": "off",
  "requiredText": ""
}
