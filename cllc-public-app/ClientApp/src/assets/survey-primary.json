{
  "title": "Find out what you need to apply for a licence",
  "pages": [
    {
      "name": "p1",
      "elements": [
        {
          "type": "radiogroup",
          "name": "age19",
          "title": "Are you 19 years old or older?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ]
    },
    {
      "name": "p1.5",
      "elements": [
        {
          "type": "radiogroup",
          "name": "licensedProducer",
          "title": "Are you a federally licensed producer of cannabis?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes'"
    },
    {
      "name": "p2",
      "elements": [
        {
          "type": "radiogroup",
          "name": "registeredBusiness",
          "title": "Do you have a registered business?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p3",
      "elements": [
        {
          "type": "radiogroup",
          "name": "businessType",
          "title": "What type of business is applying for the licence?",
          "isRequired": true,
          "choices": [
            "Corporation",
            "Partnership",
            "Sole-Proprietorship",
            "Society",
            "Local Government",
            "Indigenous Nation",
            "School/University"
          ]
        },
        {
          "type": "radiogroup",
          "name": "corporationType",
          "visible": false,
          "visibleIf": "{businessType} = 'Corporation'",
          "title": "Corporation Type",
          "isRequired": true,
          "choices": [
            {
              "value": "Public Corporation",
              "text": "Public Corporation"
            },
            {
              "value": "Private Corporation",
              "text": "Private Corporation"
            },
            {
              "value": "Limited Liability Corporation",
              "text": "Limited Liability Corporation"
            },
            {
              "value": "Unlimited Liability Corporation",
              "text": "Unlimited Liability Corporation"
            }
          ]
        },
        {
          "type": "radiogroup",
          "name": "partnershipType",
          "visible": false,
          "visibleIf": "{businessType} = 'Partnership'",
          "title": "Partnership Type",
          "isRequired": true,
          "choices": [
            {
              "value": "General Partnership",
              "text": "General Partnership"
            },
            {
              "value": "Limited Partnership",
              "text": "Limited Partnership"
            },
            {
              "value": "Limited Liability Partnership",
              "text": "Limited Liability Partnership"
            }
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No' and {registeredBusiness} = 'Yes'"
    },
    {
      "name": "p4",
      "elements": [
        {
          "type": "radiogroup",
          "name": "bceidAccount",
          "title": "Do you have a Business BCeID account?",
          "choices": [
            "Yes",
            "No",
            {
              "value": "doNotknow",
              "text": "I don't know"
            }
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No' and {registeredBusiness} = 'Yes'"
    },
    {
      "name": "p4.5",
      "elements": [
        {
          "type": "radiogroup",
          "name": "ruralRetailStore",
          "title": "Do you think you qualify for a cannabis rural retail store licence?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        },
        {
          "type": "infotext",
          "name": "note",
          "title": "A cannabis rural retail store licence is for cannabis stores in areas where the population would not support a standalone cannabis business. To qualify, the population must be no more than 2500 and the store must be at least 10km away from other cannabis stores (including proposed stores with an application in progress). ",
          "titleLocation": "hidden"
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p5",
      "elements": [
        {
          "type": "radiogroup",
          "name": "isFirstNationsLand",
          "title": "Will your proposed location be located under the jurisdiction of an Indigenous Nation?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p6",
      "elements": [
        {
          "type": "text",
          "name": "firstNationsLand",
          "visible": false,
          "visibleIf": "{isFirstNationsLand} = 'Yes'",
          "width": "50%",
          "title": "Which Indigenous Nation has jurisdiction over your proposed location?"
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p7",
      "elements": [
        {
          "type": "radiogroup",
          "name": "contactedFirstNation",
          "visible": false,
          "visibleIf": "{isFirstNationsLand} = 'Yes'",
          "title": "Have you contacted the Indigenous Nation about opening a cannabis store?",
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p8",
      "elements": [
        {
          "type": "dropdown",
          "name": "storelocation",
          "visible": false,
          "visibleIf": "{isFirstNationsLand} = 'No'",
          "width": "50%",
          "title": "Where will your store be operating?",
          "isRequired": true,
          "choicesByUrl": {
            "url": "api/jurisdiction",
            "valueName": "name"
          }
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p9",
      "elements": [
        {
          "type": "radiogroup",
          "name": "contactLg",
          "visible": false,
          "visibleIf": "{isFirstNationsLand} = 'No'",
          "title": "Have you contacted the local government about opening a cannabis store?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p10",
      "elements": [
        {
          "type": "radiogroup",
          "name": "locationOwnedLeased",
          "title": "Do you have a location currently owned or leased?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    },
    {
      "name": "p11",
      "elements": [
        {
          "type": "radiogroup",
          "name": "connectionToProducerCultivator",
          "title": "Do you or your business have any business, financial or family connections to licensed producers of cannabis?",
          "isRequired": true,
          "choices": [
            "Yes",
            "No"
          ]
        }
      ],
      "visible": false,
      "visibleIf": "{age19} = 'Yes' and {licensedProducer} = 'No'"
    }
  ],
  "showQuestionNumbers": "off",
  "requiredText": ""
}
