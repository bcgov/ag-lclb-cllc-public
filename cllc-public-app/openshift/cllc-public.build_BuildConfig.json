{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {},
    "items": [
        {
            "apiVersion": "v1",
            "kind": "ImageStream",
            "metadata": {
                "name": "cllc-public"
            }
        },
        {
            "apiVersion": "v1",
            "kind": "BuildConfig",
            "metadata": {
                "labels": {
                    "app": "cllc-public"
                },
                "name": "cllc-public"
            },
            "spec": {
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "cllc-public:latest"
                    }
                },
                "resources": {
                    "limits": {
                        "cpu": "1",
                        "memory": "6Gi"
                    },
                    "requests": {
                        "cpu": "1",
                        "memory": "4Gi"
                    }
                },
                "runPolicy": "Serial",
                "source": {
                    "contextDir": "",
                    "git": {
                        "ref": "master",
                        "uri": "https://github.com/bcgov/ag-lclb-cllc-public.git"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "sourceStrategy": {
                        "env": [
                            {
                                "name": "DOTNET_STARTUP_PROJECT",
                                "value": "cllc-public-app"
                            }
                        ],
                        "from": {
                            "kind": "DockerImage",
                            "name": "registry.access.redhat.com/dotnet/dotnet-20-rhel7:latest"
                        }
                    },
                    "type": "Source"
                },
                "triggers": [
                    {
                        "type": "ImageChange"
                    },
                    {
                        "type": "ConfigChange"
                    }
                ]
            }
        }
    ]
}
